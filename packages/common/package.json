{
  "name": "@greycat/common",
  "version": "0.0.0",
  "description": "Greycat Common API / Utils",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/typings/index.d.ts",
  "author": "DataThings S.Ã .r.l",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn compile",
    "compile": "yarn compile:cjs && yarn compile:esm && yarn compile:typings",
    "compile:cjs": "ncc -m build src/index.ts -o dist/cjs",
    "compile:esm": "tsc -b tsconfig.esm.json",
    "compile:typings": "tsc -b tsconfig.typings.json",
    "test": "tslint --project '.'",
    "prepackage": "yarn version --new-version ${VERSION:-0.0.0} --no-git-tag-version",
    "package": "yarn pack --filename ${DIST_DIR:-dist}/generic/ts/greycat-common.tgz"
  },
  "dependencies": {
    "@greycat/types": "0.0.0"
  }
}
