{
  "name": "@greycat${TS_SCOPE_PREFIX}/cli",
  "version": "${GREYCAT_VERSION}",
  "description": "Greycat CLI",
  "author": "DataThings S.Ã .r.l",
  "license": "MIT",
  "main": "./lib/greycat-core.js",
  "bin": {
    "greycat": "./bin/greycat",
    "greycat-js": "./lib/greycat.js"
  },
  "scripts": {
    "prebuild": "tslint --project '.'",
    "build": "yarn clean && mkdir -p bin && yarn compile",
    "compile": "tsc -b tsconfig.build.json",
    "postcompile": "chmod +x lib/greycat.js lib/commands/*",
    "watch": "tsc -b tsconfig.build.json --watch",
    "pretest": "rm -rf fixtures/*.db",
    "test": "mocha lib/**/*.test.js --full-trace",
    "clean": "rm -rf lib",
    "package": "yarn pack --filename greycat-cli.tgz"
  },
  "dependencies": {
    "@greycat${TS_SCOPE_PREFIX}/core-napi": "${GREYCAT_VERSION}",
    "chalk": "2.4.2",
    "commander": "3.0.2",
    "debug": "4.1.1",
    "fs-extra": "8.1.0"
  },
  "devDependencies": {
    "@types/node": "12.12.23",
    "@types/debug": "4.1.5",
    "@types/fs-extra": "8.0.0"
  }
}
