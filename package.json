{
  "name": "@greycat/sdk",
  "version": "0.0.0",
  "type": "module",
  "module": "dist/esm/index.js",
  "typings": "dist/typings/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/typings/index.d.ts",
      "import": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist/esm"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run build:esm && pnpm run build:bundle",
    "build:esm": "tsc -b tsconfig.json",
    "build:bundle": "esbuild --bundle src/index.ts --tsconfig=tsconfig.json --outfile=dist/bundle/greycat.sdk.min.js --minify --sourcemap",
    "fmt": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint src  --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "prepare:test": "rm -rf gcdata && greycat run && cp gcdata/store/abi project.test.abi",
    "test": "pnpm run build:esm && node --test"
  },
  "author": "DataThings s.a <contact@datathings.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "esbuild": "0.17.11",
    "eslint": "8.35.0",
    "eslint-plugin-require-extensions": "0.1.2",
    "prettier": "2.8.4",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@7.9.5",
  "publishConfig": {
    "@greycat:registry": "https://hub.datathings.com/api/v4/projects/289/packages/npm/"
  }
}